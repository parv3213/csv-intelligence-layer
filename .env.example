# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/csv_intelligence

# Redis
REDIS_URL=redis://localhost:6379

# Storage (local filesystem for dev, S3 for production)
STORAGE_TYPE=local
STORAGE_PATH=./storage
# S3 config (when STORAGE_TYPE=s3)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# S3_BUCKET=csv-intelligence

# OpenAI (optional - for AI-assisted column mapping)
OPENAI_API_KEY=

# Pipeline settings
INFERENCE_SAMPLE_SIZE=1000
MAPPING_CONFIDENCE_THRESHOLD=0.8
MAX_FILE_SIZE_MB=100
